algoritmo "Desconto"
// Função :
// Autor :
// Data : 21/03/2014
// Seção de Declarações 
var
      preco,fin:real
inicio
      Escreva("Escreva o preço do produto a receber desconto de 10%:")
      Leia(preco)
      
      // Seção de Comandos
      fin<-preco-preco*0.1
      // Saída
      Escreval("O preço final do produto é:",fin)
      
fimalgoritmo
