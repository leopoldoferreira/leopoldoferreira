algoritmo "Média Ponderada"
// Função :
// Autor :
// Data : 3/21/2014
// Seção de Declarações 
var
      nota1,nota2,media:real
inicio
      Escreva("Digite duas notas sendo a primeira peso 2 e segunda peso 3:")
      Leia(nota1,nota2)
      // Seção de Comandos
      media<-(nota1*2+nota2*3)/5
      // Saída
      Escreval("Média:",media)
      
fimalgoritmo
