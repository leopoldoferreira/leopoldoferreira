algoritmo "Multiplicação"
// Função :
// Autor :
// Data : 3/21/2014
// Seção de Declarações 
var
n1,n2,n3,mult:real
inicio
      Escreva("Escreva três números a serem multiplicado:")
      Leia(n1,n2,n3)
      // Seção de Comandos
      mult<-n1*n2*n3
      // Saída
      Escreval("Multiplicação:",mult)
      
fimalgoritmo
