algoritmo "Ângulo"
// Função :
// Autor :
// Data : 3/26/2014
// Seção de Declarações 
var
      a1,a2,a3,at:real
inicio
      Escreva("Digite dois angulos de um triângulo:")
      Leia(a1,a2)
      // Seção de Comandos
      at<-180
      a3<-at-(a1+a2)
      // Saída
      Escreval("O terceiro ângulo é de:",a3)
      
fimalgoritmo
