algoritmo "Quadrado"
// Função :
// Autor :
// Data : 3/22/2014
// Seção de Declarações 
var
      area,lado:real
inicio
      Escreva("Digite o lado do quadrado:")
      Leia(lado)
      // Seção de Comandos
      area<-lado*lado
      // Saída
      Escreval("A área do quadrado é:",area)
      
fimalgoritmo
