algoritmo "Salário Mínimo"
// Função :
// Autor :
// Data : 25/03/2014
// Seção de Declarações 
var
      sm,sf,qsm:real
inicio
      Escreva("Digite o salário mínimo atual:")
      Leia(sm)
      Escreva("Digite o valor do seu salário:")
      Leia(sf)
      // Seção de Comandos
      qsm<-sf/sm
      // Saída
      Escreval("Você recebe",qsm," salários mínimos")
      
fimalgoritmo
